    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.5.4",
    "ember-source": "3.0.0",
    "express-basic-auth": "^1.1.4",
    "express-force-ssl": "^0.3.2",
    "fastboot-app-server": "ember-fastboot/fastboot-app-server#1c253be53c1263c26a3c80abd6511188a04137d3",
    "forcedomain": "^0.8.1",
    "graphql": "0.13.1",
    "loader.js": "^4.6.0",
    "raven": "2.4.2",
    "simple-css-reset": "2.0.6"
  },
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "ember-cli-chai": "^0.4.3",
    "ember-cli-mocha": "0.15.0",
    "ember-exam": "1.0.0",
    "eslint": "4.18.2",
    "eslint-plugin-ember": "5.1.0",
