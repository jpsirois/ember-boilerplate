{
  "name": "ember-boilerplate",
  "version": "0.0.1",
  "description": "Small description for ember-boilerplate goes here",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "server": "node app/server.js",
    "start": "ember server",
    "build": "ember build",
    "test": "COVERAGE=true ember exam --split 5 --parallel",
    "check-coverage": "./node_modules/.bin/nyc check-coverage",
    "svgo": "./node_modules/.bin/svgo --config=.svgo.yml",
    "lint-scripts": "./node_modules/.bin/eslint './app/**/*.js' './config/**/*.js' './mirage/**/*.js' './public/**/*.js' './tests/**/*.js' './ember-cli-build.js' './testem.js'",
    "lint-styles": "./node_modules/.bin/stylelint './app/**/*.scss'",
    "lint": "npm run lint-scripts && npm run lint-styles"
  },
  "repository": "https://github.com/mirego/ember-boilerplate",
  "engines": {
    "node": "^8.10.0",
    "npm": "^5.6.0"
  },
  "dependencies": {
    "apollo-cache-inmemory": "1.1.10",
    "apollo-client": "2.2.6",
    "apollo-link": "1.2.1",
    "apollo-link-http": "1.5.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "broccoli-asset-rev": "^2.6.0",
    "ember-cli": "3.0.1",
    "ember-cli-autoprefixer": "^0.8.1",
    "ember-cli-babel": "6.12.0",
    "ember-cli-code-coverage": "github:charlesdemers/ember-cli-code-coverage#6961be288d5666e0c8eae4d1988bbfac3773b433",
    "ember-cli-content-security-policy": "^1.0.0",
    "ember-cli-dependency-checker": "2.1.0",
