{
  "name": "ember-boilerplate",
  "version": "0.0.1",
  "description": "Small description for ember-boilerplate goes here",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "server": "NODE_PATH=$NODE_PATH:. node --optimize_for_size app/server.js",
    "start": "ember server",
    "build": "ember build",
    "test": "COVERAGE=true ember exam --split 5 --parallel",
    "check-coverage": "./node_modules/.bin/nyc check-coverage",
    "svgo": "./node_modules/.bin/svgo --config=.svgo.yml",
    "prettier": "prettier --write ember-cli-build.js testem.js '{app,tests,config}/**/*.{js,graphql,scss}'",
    "prettier-check": "prettier -l ember-cli-build.js testem.js '{app,tests,config}/**/*.{js,graphql,scss}'",
    "lint-scripts": "./node_modules/.bin/eslint ember-cli-build.js testem.js app/ tests/ config/",
    "lint-styles": "./node_modules/.bin/stylelint './app/**/*.scss'",
    "lint-templates": "./node_modules/.bin/ember-template-lint './app/**/*.hbs'"
  },
  "repository": "https://github.com/mirego/ember-boilerplate",
  "engines": {
    "node": "10.4.1",
    "npm": "6.1.0"
  },
  "dependencies": {
    "@ember-decorators/argument": "0.8.16",
    "@ember/optional-features": "0.6.1",
    "apollo-cache-inmemory": "1.2.5",
    "apollo-client": "2.3.5",
    "apollo-link-context": "1.0.8",
    "apollo-link-http": "1.5.4",
    "babel-plugin-graphql-tag": "1.6.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "broccoli-asset-rev": "2.7.0",
    "compression": "1.7.2",
    "ember-angle-bracket-invocation-polyfill": "1.1.5",
    "ember-apollo-client": "1.0.1",
    "ember-cli": "3.1.4",
    "ember-cli-autoprefixer": "0.8.1",
    "ember-cli-babel": "6.14.1",
    "ember-cli-code-coverage": "github:charlesdemers/ember-cli-code-coverage#6961be288d5666e0c8eae4d1988bbfac3773b433",
    "ember-cli-content-security-policy": "1.0.0",
    "ember-cli-dependency-checker": "2.1.1",
    "ember-cli-fastboot": "1.1.4-beta.1",
    "ember-cli-graphql-file": "0.0.1",
    "ember-cli-htmlbars": "2.0.3",
    "ember-cli-htmlbars-inline-precompile": "1.0.3",
    "ember-cli-inject-live-reload": "1.7.0",
    "ember-cli-sass": "7.1.7",
    "ember-cli-sentry": "3.0.0",
    "ember-cli-shims": "1.2.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-uglify": "2.1.0",
    "ember-concurrency": "0.8.19",
    "ember-css-modules": "0.7.10",
    "ember-css-modules-sass": "1.0.0",
    "ember-decorators": "2.2.0",
    "ember-fetch": "5.0.0",
    "ember-inline-svg": "0.1.11",
    "ember-intl": "3.0.0",
    "ember-load-initializers": "1.1.0",
    "ember-resolver": "4.5.6",
    "ember-source": "3.2.2",
    "express-basic-auth": "1.1.5",
    "express-force-ssl": "0.3.2",
    "fastboot-app-server": "1.1.2-beta.1",
    "forcedomain": "1.0.0",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "loader.js": "4.7.0",
    "raven": "2.6.3",
    "simple-css-reset": "2.0.6"
  },
  "devDependencies": {
    "@ember-decorators/babel-transforms": "2.0.1",
    "babel-eslint": "8.2.5",
    "ember-auto-import": "1.0.1",
    "ember-cli-chai": "0.5.0",
    "ember-cli-mocha": "0.15.0",
    "ember-exam": "1.0.0",
    "ember-mocha": "0.14.0",
    "ember-native-dom-helpers": "0.6.2",
    "ember-sinon": "2.1.0",
    "ember-template-lint": "0.8.20",
    "ember-window-mock": "0.4.1",
    "eslint": "5.0.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-ember": "5.2.0",
    "eslint-plugin-mirego": "0.0.1",
    "lint-staged": "7.2.0",
    "nyc": "12.0.2",
    "prettier": "1.13.7",
    "sinon-chai": "3.2.0",
    "stylelint": "9.3.0",
    "stylelint-config-mirego": "1.0.1",
    "stylelint-order": "0.8.1",
    "svgo": "1.0.5"
  }
}
